<article itemscope itemtype="https://schema.org/Guide" class="card resource" data-filter="{{ resource.data.type | slug }} {{ resource.data.buildingBlock | slug }}">
    <h2 itemprop="name" class="h4"><a href="{{ resource.url }}">{{ resource.data.title }}</a></h2>
    <p class=""><strong>{{ resource.data.type }}</strong> for&nbsp;<br /><strong>{{ resource.data.buildingBlock }}</strong></p>
</article>
    